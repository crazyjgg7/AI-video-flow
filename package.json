{
    "name": "ai-video-flow",
    "version": "0.1.0",
    "private": true,
    "description": "AI 短视频自动生成平台",
    "workspaces": [
        "frontend"
    ],
    "scripts": {
        "dev:frontend": "npm run dev -w frontend",
        "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\""
    },
    "devDependencies": {
        "concurrently": "^8.2.0"
    }
}